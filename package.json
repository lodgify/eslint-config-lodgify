{
  "name": "@lodgify/eslint-config",
  "description": "ESLint config for Lodgify's projects.",
  "version": "0.0.0-development",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lodgify/eslint-config.git"
  },
  "main": "index.js",
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "scripts": {
    "lint": "eslint index.js rule-options/*",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,css,json,md}\"",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "babel-eslint": "^10.0.1",
    "eslint-config-prettier": "^4.3.0",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsdoc": "^7.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "babel-plugin-module-resolver": "^3.2.0",
    "eslint": "^5.16.0",
    "prettier": "^1.18.2",
    "semantic-release": "^17.2.3",
    "travis-deploy-once": "^5.0.11"
  },
  "peerDependencies": {
    "@babel/core": "^7.4.5",
    "babel-plugin-module-resolver": "^3.2.0",
    "eslint": "^5.16.0",
    "prettier": "^1.18.2"
  }
}
